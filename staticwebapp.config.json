{
  "auth": {
    "login": {
      "routes": [
        {
          "route": "/*",
          "roles": ["anonymous"]
        }
      ]
    }
  },
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/*.css", "/*.js"]
  }
}
