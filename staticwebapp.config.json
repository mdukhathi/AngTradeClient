{
  "auth": {
    "identityProviders": {
      "github": {
        "registration": {
          "enabled": false
        }
      }
    }
  },
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/*.css", "/*.js"]
  }
}
